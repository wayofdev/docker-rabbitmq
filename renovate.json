{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base"
    ],
    "automerge": true,
    "platformAutomerge": true,
    "regexManagers": [
        {
            "fileMatch": [
                "inventory.yml$"
            ],
            "matchStrings": [
                "(?<depName>^\\s*rabbitmq_version):\\s*(?<currentValue>\\S+)"
            ],
            "datasourceTemplate": "docker"
        }
    ]
}

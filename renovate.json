{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base"
    ],
    "automerge": true,
    "platformAutomerge": true,
    "regexManagers": [
        {
            "fileMatch": [
                "^src/inventory\\.yml$"
            ],
            "matchStrings": [
                "^\\s*rabbitmq_version:\\s*(?<currentValue>\\S+)"
            ],
            "depNameTemplate": "docker",
            "datasourceTemplate": "rabbitmq"
        }
    ]
}
